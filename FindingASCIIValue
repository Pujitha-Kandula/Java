import java.util.*;

public class Main
{
	public static void main(String[] args) {
	    Scanner sc=new Scanner(System.in);
		System.out.println("Enter a character: ");
		char s=sc.next().charAt(0);
		int castASCII=s;
		System.out.printf("The ASCII value of character is "+castASCII);
		sc.close();
	}
}
